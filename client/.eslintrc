{
  "env": {
    "browser": true
  },
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "prefer-arrow-callback": "off",
    "react/jsx-no-bind": "off",
    "react/prefer-stateless-function": [1],
    "react/prop-types": "off",
    "jsx-a11y/label-has-associated-control": "off"
  }
}
